@{
    ViewBag.Title = "视频媒体库 - 明星代言 - 大奖娱乐官方网站";
    Layout = "~/Views/Shared/_CNLayout.cshtml";
    string ImageServer = ViewContext.ViewData["ImageServer"].ToString();

}
@section AdditionalStyles {
    <link rel="stylesheet" href="@ImageServer/Content/Styles/endorsement.css" />
}
@section AdditionalScripts {
    <script type="text/javascript" src="@ImageServer/Scripts/Custom/main.js"></script>
    <script type="text/javascript" src="@ImageServer/Scripts/Plugins/div-slider-1.0.3.js"></script>
    <script type="text/javascript" src="@ImageServer/Scripts/Custom/endorsement-video.js"></script>   
}
<div id="wrapper">
    <div class="container">
        <div  class="leftRightContent videoLRContent">
            <div class="topContent">
                <div class="floatLeft videoHead">
                    <img src="@ImageServer/Content/Images/endorsement/video_title.jpg" /><a href="@Url.Action("Endorsement", "CN")" target="_self"><img src="@ImageServer/Content/Images/endorsement/gallery_back.jpg" /></a>
                </div>
                <div class="floatLeft videoList">
                    <ul class="thumbnailList">
                        <li class="current"><img src="@ImageServer/Content/Images/endorsement/video_thumbnail_5.jpg" /><img src="@ImageServer/Content/Images/endorsement/overlay.png" class="overlay" /></li>
                        <li><img src="@ImageServer/Content/Images/endorsement/video_thumbnail_4.jpg" /><img src="@ImageServer/Content/Images/endorsement/overlay.png" class="overlay" /></li>
                    </ul>
                    <div class="clearfix"></div>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="mainContent">
                <div class="video-container current"><iframe src="http://static.youku.com/v/swf/qplayer.swf?VideoIDS=XNjIzNDMzMTQw=&isAutoPlay=true&isShowRelatedVideo=false&embedid=-&showAd=0" width="999" height="594" frameborder="0" webkitAllowFullScreen mozallowfullscreen allowFullScreen></iframe></div>
                <div class="video-container"><iframe src="http://player.youku.com/player.php/sid/XNjIzNDk5NDMy/v.swf" width="999" height="594" frameborder="0" webkitAllowFullScreen mozallowfullscreen allowFullScreen></iframe> </div>
            </div>
            <div></div>
        </div>
    </div>
</div>                                      